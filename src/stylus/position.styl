// 定位属性
.p-r
  position: relative!important
.p-a
  position absolute!important
.p-f
  position fixed!important
.p-s
  position static!important

// 权重属性
.z-1
  z-index 1
.z-5
  z-index 5
.z-10
  z-index 10
.z-15
  z-index 15
.z-20
  z-index 20
.z-100
  z-index 100
.z-200
  z-index 200
.z-1000
  z-index 1000
.z-10000
  z-index 10000

// 位置属性-单元
.pos-l-0
  left 0
.pos-l-32
  left 32px
.pos-l-50
  left 50%
.pos-r-0
  right 0
.pos-r-50
  right 50%
.pos-t-0
  top 0
.pos-t-100
  top 100
.pos-t-36
  top 36px
.pos-t-50
  top 50%
// .pos-t-100
  // top 100px
.pos-t-100
  top 100px
.pos-b-0
  bottom 0
.pos-b-96
  bottom 96px
.pos-b-50
  bottom 50%

.p-translate-50
  transform: translate(-50%,-50%)
.p-translateX-50
  transform: translateX(-50%)
.p-translateY-50
  transform: translateY(-50%)

// 位置属性-综合-absolute
.p-a-c //----absolute双向居中
  @extend .p-a
  @extend .pos-l-50
  @extend .pos-t-50
  @extend .p-translate-50

.p-a-xl-yc
  @extend .p-a
  @extend .pos-l-0
  @extend .pos-t-50
  @extend .p-translateY-50
.p-a-xr-yc
  @extend .p-a
  @extend .pos-r-0
  @extend .pos-t-50
  @extend .p-translateY-50
.p-a-xc-yt
  @extend .p-a
  @extend .pos-l-50
  @extend .pos-t-0
  @extend .p-translateX-50
.p-a-xc-yb
  @extend .p-a
  @extend .pos-l-50
  @extend .pos-b-0
  @extend .p-translateX-50
.p-a-xl-yt
  @extend .p-a
  @extend .pos-l-0
  @extend .pos-t-0
.p-a-xl-yb
  @extend .p-a
  @extend .pos-l-0
  @extend .pos-b-0
.p-a-xr-yt
  @extend .p-a
  @extend .pos-r-0
  @extend .pos-t-0
.p-a-xr-yb
  @extend .p-a
  @extend .pos-r-0
  @extend .pos-b-0

// 绝对定位
.p-f-c //----absolute双向居中
  @extend .p-f
  @extend .pos-l-50
  @extend .pos-t-50
  @extend .p-translate-50

.p-f-xl-yc
  @extend .p-f
  @extend .pos-l-0
  @extend .pos-t-50
  @extend .p-translateY-50
.p-f-xr-yc
  @extend .p-f
  @extend .pos-r-0
  @extend .pos-t-50
  @extend .p-translateY-50
.p-f-xc-yt
  @extend .p-f
  @extend .pos-l-50
  @extend .pos-t-0
  @extend .p-translateX-50
.p-f-xc-yb
  @extend .p-f
  @extend .pos-l-50
  @extend .pos-b-0
  @extend .p-translateX-50
.p-f-xl-yt
  @extend .p-f
  @extend .pos-l-0
  @extend .pos-t-0
.p-f-xl-yb
  @extend .p-f
  @extend .pos-l-0
  @extend .pos-b-0
.p-f-xr-yt
  @extend .p-f
  @extend .pos-r-0
  @extend .pos-t-0
.p-f-xr-yb
  @extend .p-f
  @extend .pos-r-0
  @extend .pos-b-0
