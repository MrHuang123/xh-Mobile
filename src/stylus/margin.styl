// 会生成数组中的margin-top、margin-bottom、margin-left、margin-right等类
/*
  $range-margin,中每一个数字都会生产该数字下的7个类名，用继承性，机会不用担心冗余
  如数字5下7个类名分别为：
  .m-t-5{margin-top:5px}
  .m-b-5{margin-bottom:5px}
  .m-l-5{margin-left:5px}
  .m-r-5{margin-right:5px}
  .m-tb-5{margin-top:5px;margin-bottom:5px;}
  .m-lr-5{margin-left:5px;margin-right:5px;}
  .m-5{margin-top:5px;margin-bottom:5px;margin-left:5px;margin-right:5px;}

*/ 

// 注意：最后一位数不要加逗号

$range-margin = 0,
                2,
                4,
                5,
                8,
                10, 
                12,
                14,
                15,
                16,
                18,
                20,
                22,
                23,
                24,
                25,
                28, 
                30,
                -30,
                32,
                35, 
                40,
                42,
                48, 
                45, 
                50, 
                55, 
                60,
                62, 
                65, 
                70, 
                75, 
                80,
                82, 
                85, 
                90,
                94,
                95,
                96,
                100,
                104,
                110,
                120,
                122,
                140,
                150,
                200,
                280


for margin in $range-margin
  .m-t-{margin}
    margin-top (margin)px!important
  .m-l-{margin}
    margin-left (margin)px!important
  .m-b-{margin}
    margin-bottom (margin)px!important
  .m-r-{margin}
    margin-right (margin)px!important
  .m-tb-{margin}
    @extend .m-t-{margin}
    @extend .m-b-{margin}
  .m-lr-{margin}
    @extend .m-l-{margin}
    @extend .m-r-{margin}
  .m-{margin}
    @extend .m-t-{margin}
    @extend .m-b-{margin}
    @extend .m-l-{margin}
    @extend .m-r-{margin}

.m-a
  margin 0 auto