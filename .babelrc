{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 20 versions", "not ie <= 8"],
        "node":"current"
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "transform-vue-jsx", 
    "transform-runtime",
    ["import", 
      { 
        "libraryName": "vant",
        "style": true
      },
    ],
    ["import", 
      { 
        "libraryName": "ant-design-vue",
        "libraryDirectory": "lib",
        "style": "css"
      }
    ]
  ]
}
